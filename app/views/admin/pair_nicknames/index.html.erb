<h1><%= t PairNickname.model_name.human(count: 2) %></h1>
<table class='table table-hover'>
  <thead>
      <th><%= PairNickname.human_attribute_name(:pair) %></th>
      <th><%= PairNickname.human_attribute_name(:name) %></th>
      <th><%= t 'status.status' %></th>
      <th><%= t 'actions.actions' %><th>
  </thead>

  <tbody>
    <% @pair_nicknames.each do |pair_nickname| %>
      <tr>
        <td><%= pair_nickname.pair.name %></td>
        <td><%= pair_nickname.name %></td>
        <td><%= status_mark pair_nickname.status %></td>
        <td><%= render 'admin/shared/index_buttons', resource: pair_nickname %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<div class='row'>
  <div class='col-md-2'>
    <%= link_to t('actions.create'), new_admin_pair_nickname_path, class: 'btn btn-primary btn-sm' %>
  </div>
</div>
